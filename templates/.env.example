# Wolf Prowler Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/wolf_prowler
WOLF_DATABASE_URL=postgresql://username:password@localhost:5432/wolf_prowler

# API Keys
NVD_API_KEY=your_nvd_api_key_here
VT_API_KEY=your_virustotal_api_key_here
WOLF_PROWLER_API_KEY=your_wolf_prowler_api_key_here
WOLF_API_KEY=your_wolf_api_key_here

# LLM Configuration
WOLF_LLM_API_URL=http://host.docker.internal:11434/api/generate

# Security Credentials
WOLF_ADMIN_USERNAME=admin
WOLF_ADMIN_PASSWORD=change_this_secure_password
WOLF_SECRET_KEY=generate_a_64_byte_hex_key_here
